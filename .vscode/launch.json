{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Console (UCRT64)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/ilctool.exe",
      "cwd": "${workspaceFolder}",
      "MIMode": "gdb",
      "miDebuggerPath": "C:/msys64/ucrt64/bin/gdb.exe",
      "preLaunchTask": "Build (UCRT64) - Console",
      "externalConsole": false,
      "stopAtEntry": false,
      "environment": [
        {
          "name": "PATH",
          "value": "C:\\msys64\\ucrt64\\bin;C:\\msys64\\usr\\bin;${env:PATH}"
        }
      ],
      "setupCommands": [
        {
          "description": "Enable pretty printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}
